<div class="j-slider"></div>
<div class="j-scale-code b-scale-code">
    <ul>
    <% var value = dateScale.getScale();%>
    <% _.each(dateScale.getAvailableScales(), function(code) { %>
    <li class="j-code-value <%if(code == value.code) {%>b-selected<%}%>" data-code="<%=code%>"><%=code%></li>
    <% }) %>
    </ul>
</div>